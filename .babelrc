{
    "presets":[
        "react",
        "es2015"
    ],
    "plugins":[
      "transform-decorators-legacy",
      "add-module-exports"
    ],

    "env": {
        "development": {
          "plugins": [["react-transform", {
            "transforms": [{
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }]
          }]]
        }
      }

}